{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "rootDir": "./src",
        "outDir": "./dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["./src/**/*.ts", "./src/**/*.d.ts"],
    "exclude": ["./node_modules"]
}
